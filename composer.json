{
   "name":"phpdune/framework",
   "description":"The PHP Dune Framework !",
   "type":"library",
   "license":"MIT",
   "keywords":[
      "php framework",
      "framework",
      "Dune"
   ],
   "authors":[
      {
         "name":"coderswat",
         "email":"phpdune@gmail.com"
      }
   ],
   "autoload":{
      "psr-4":{
         "Dune\\":"src/Dune/"
      },
      "files":[
         "src/Dune/Helpers/functions.php"
      ]
   },
   "autoload-dev":{
      "psr-4":{
         "Dune\\Tests\\":"tests/"
      }
   },
   "require":{
      "php":"^8.0",
      "symfony/var-dumper":"^6.2",
      "php-di/php-di": "^7.0",
      "symfony/console": "^6.2"
   },
   "require-dev":{
      "phpunit/phpunit":"^10.1",
      "phpstan/phpstan": "^1.10"
   }
}
